{
  "name": "tourze/campaign-credit-bundle",
  "description": "Credit reward support for campaign-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "psr/log": "^3|^2|^1",
    "symfony/http-kernel": "^7.3",
    "symfony/security-core": "^7.3",
    "symfony/uid": "^7.3",
    "tourze/campaign-bundle": "^1.0",
    "tourze/credit-bundle": "1.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/dependency-injection": "^7.3",
    "symfony/phpunit-bridge": "^7.3"
  },
  "autoload": {
    "psr-4": {
      "CampaignCreditBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CampaignCreditBundle\\Tests\\": "tests/"
    }
  }
}